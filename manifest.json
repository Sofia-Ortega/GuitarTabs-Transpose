{
  "manifest_version": 3,
  "name": "Guitar Tabs Transposer",
  "version": "0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://tabs.ultimate-guitar.com/tab/*"],
  "content_scripts": [
    {
      "js": ["assets/js/content.js"],
      "matches": ["https://tabs.ultimate-guitar.com/tab/*"]
    }
  ]
}
